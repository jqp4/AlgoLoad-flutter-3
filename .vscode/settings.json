        "**/*.g.dart",
        "**/*.gr.dart",
        "**/*.gen.dart",
        "**/*.part.dart",
        "**/*.config.dart",
        "**/*_ignore.dart",
        "**/*_event.dart",
        "**/*_state.dart"
    ]

